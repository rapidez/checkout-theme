{
    "name": "rapidez/checkout-theme",
    "description": "Rapidez Checkout Theme",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Roy Duineveld",
            "email": "royduineveld@gmail.com",
            "homepage": "https://royduineveld.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "rapidez/account": "^5.0",
        "rapidez/core": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\CheckoutTheme\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\CheckoutTheme\\ServiceProvider"
            ]
        }
    }
}
